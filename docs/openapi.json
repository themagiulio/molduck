{"openapi": "3.1.0", "info": {"title": "NinjaAPI", "version": "1.0.0", "description": ""}, "paths": {"/api/v1/": {"get": {"operationId": "molduck_api_version", "summary": "Version", "parameters": [], "responses": {"200": {"description": "OK"}}, "description": "Retrieve webserver version."}}, "/api/v1/jobs/": {"get": {"operationId": "molduck_api_list_jobs", "summary": "List Jobs", "parameters": [], "responses": {"200": {"description": "OK", "content": {"application/json": {"schema": {"items": {"$ref": "#/components/schemas/Job"}, "title": "Response", "type": "array"}}}}}, "description": "List all jobs."}, "post": {"operationId": "molduck_api_create_job", "summary": "Create Job", "parameters": [], "responses": {"200": {"description": "OK", "content": {"application/json": {"schema": {"$ref": "#/components/schemas/Job"}}}}}, "description": "Create a new job.", "requestBody": {"content": {"application/json": {"schema": {"$ref": "#/components/schemas/QCSchemaInput"}}}, "required": true}}}, "/api/v1/jobs/{job_id}/": {"get": {"operationId": "molduck_api_retrieve_job", "summary": "Retrieve Job", "parameters": [{"in": "path", "name": "job_id", "schema": {"format": "uuid", "title": "Job Id", "type": "string"}, "required": true}], "responses": {"200": {"description": "OK", "content": {"application/json": {"schema": {"$ref": "#/components/schemas/Job"}}}}}, "description": "Retrieve a job."}}, "/api/v1/jobs/{job_id}/qcschema/": {"get": {"operationId": "molduck_api_retrieve_job_as_qcschema", "summary": "Retrieve Job As Qcschema", "parameters": [{"in": "path", "name": "job_id", "schema": {"format": "uuid", "title": "Job Id", "type": "string"}, "required": true}], "responses": {"200": {"description": "OK"}}, "description": "Retrieve a job input and output as QC Schema."}}}, "components": {"schemas": {"Job": {"properties": {"uuid": {"format": "uuid", "title": "Uuid", "type": "string"}, "success": {"title": "Success", "type": "boolean"}, "input_data": {"$ref": "#/components/schemas/QC